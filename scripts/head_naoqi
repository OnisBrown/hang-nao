#!/usr/bin/env python2.7

import sys
import naoqi
from naoqi import ALProxy
import time
import almath
import rospy
import roslib
from trajectory_msgs.msg import JointTrajectory, JointTrajectoryPoint

p = rospy.Publisher('nao_dcm/Head_controller/command', JointTrajectory, queue_size = 100)
jt = JointTrajectory()
jt.points.positions =0.0;                 
jt.points.velocities =1.0;
jt.points.accelerations =0.0;
jt.points.effort =0.0;
p.publish(jt)
